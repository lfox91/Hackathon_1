{
  "manifest_version": 2,
  "name": "Procrastinator",
  "version": "1.0",
  
  "default_local": "en",
  "description": "anti-procrastination tool",
  "icon": {
  	"16": "icon.png",
  	"48":	"icon.png",
  	"128": "icon.png"
  },

  "browser_action":{
  	"default_icon": {
  		"20": "images/icon12.png"
  	},
  	"defult_title": "Procrastinator",
  	"default_popup": "popup.html"
  },

  "background": {
    "page": "background.html",
    "persistent": true
  },
 
  "options_page": "options.html",

  "permissions": [
  	"activeTab",
  	"alarms",
  	"contentSettings",
  	"contextMenus",
  	"notifications",
  	"storage",
  	"tabs",
  	"wallpaper",
  	"webNavigation",
  	"webRequest",
  	"webRequestBlocking"
  	],

   "version_name": "1.0 beta"
}

